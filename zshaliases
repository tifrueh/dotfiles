# Permanent aliases are put here:
alias zshconfig="$EDITOR ~/.zshrc"
alias aliasconfig="$EDITOR ~/.zshaliases"
alias vimconfig="$EDITOR ~/.vimrc"
alias nethackconfig="$EDITOR ~/.nethackrc"
alias l="ls -lhF"
alias la="ls -lAhF"
alias q="exit"
alias nao="ssh nethack@alt.org"
alias sha256="shasum --algorithm 256"
