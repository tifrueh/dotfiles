# open terminal
ctrl + alt - return : kitty --single-instance --instance-group "skhd" --directory "~"

# configure yabai focus movement
ctrl + alt - up : yabai -m window --focus north
ctrl + alt - right : yabai -m window --focus east
ctrl + alt - down : yabai -m window --focus south
ctrl + alt - left : yabai -m window --focus west
ctrl + alt + shift - up : yabai -m window --focus first

# toggle float
ctrl + alt - v : yabai -m window --toggle float
