# Permanent aliases are put here:
alias l="ls -lhF --color=auto"
alias la="ls -lAhF --color=auto"
alias grep="grep --color=auto"
alias sha256="shasum --algorithm 256"
alias ga="git add"
alias gc="git commit -v"
alias gp="git push"
alias gl="git log --oneline --graph"
alias glf="git log --color=always --oneline | fzf --layout=reverse --preview='git show --color=always {1}' --ansi --accept-nth=1 --no-sort"
alias nvman="man -P 'nvim +Man!'"
